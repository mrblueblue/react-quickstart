{
  "name": "react-quickstart",
  "version": "1.0.0",
  "description": "a quick and minimal react starter kit",
  "main": "src/app.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/mrblueblue/react-quickstart"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build:webpack": "NODE_ENV=production webpack --config webpack.config.prod.js",
    "build": "npm run clean && npm run build:webpack",
    "start": "node devServer.js",
    "lint": "eslint src",
    "test": "mocha --reporter nyan --compilers js:babel/register --recursive"
  },
  "keywords": [
    "react",
    "es6",
    "webpack",
    "starter"
  ],
  "author": "Jonathan Huang <jon@levity.io>",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^5.4.7",
    "babel-eslint": "^3.1.9",
    "babel-loader": "^5.1.2",
    "babel-plugin-react-transform": "^1.1.1",
    "chai": "^3.4.1",
    "css-loader": "^0.23.0",
    "eslint": "^1.3.1",
    "eslint-plugin-react": "^2.3.0",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^0.9.1",
    "gulp": "^3.9.0",
    "gulp-rename": "^1.2.2",
    "gulp-template": "^3.1.0",
    "mocha": "^2.3.3",
    "node-sass": "^3.4.2",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.0",
    "redbox-react": "^1.0.1",
    "rimraf": "^2.4.3",
    "sass-loader": "^3.1.1",
    "style-loader": "^0.13.0",
    "webpack": "^1.9.6",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-hot-middleware": "^2.0.0",
    "yargs": "^3.30.0"
  },
  "dependencies": {
    "react": "^0.14.2",
    "react-dom": "^0.14.2"
  }
}
